@font-face {
	font-family: 'bwblog';
	src:url('../fonts/bwblog.eot');
	src:url('../fonts/bwblog.eot?#iefix') format('embedded-opentype'),
		url('../fonts/bwblog.woff') format('woff'),
		url('../fonts/bwblog.ttf') format('truetype'),
		url('../fonts/bwblog.svg#bwblog') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'bwblog';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-home:before {
	content: "\e600";
}
.icon-edit:before {
	content: "\e601";
}
.icon-options:before {
	content: "\e602";
}
.icon-tag:before {
	content: "\e603";
}
.icon-category:before {
	content: "\e604";
}
.icon-time:before {
	content: "\e605";
}
.icon-ok:before {
	content: "\e606";
}
.icon-dashboard:before {
	content: "\e607";
}
