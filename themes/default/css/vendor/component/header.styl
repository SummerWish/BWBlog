.page-header
    background      url(../img/header-bg.png) no-repeat center center
    height          400px

.page-header-title
    text-align      center
    margin-top      230px

.page-header-title a, .page-header-title a:visited, .page-header-title a:hover, .page-header-title a:active
    color           #FFF
    text-shadow     1px 1px 3px rgba(0,0,0,0.3)
    text-decoration none